{
  "name": "Skibidi accents",
  "description": "Type French accents quickly with CTRL + SPACE.",
  "version": "1.0",
  "manifest_version": 3,
  
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"],
      "css": ["style.css"],
      "all_frames": true
    }
  ],
  
  "action": {
    "default_popup": "settings.html",
    "default_icon": "logo.png"
  }
}
